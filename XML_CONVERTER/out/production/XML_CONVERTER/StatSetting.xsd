<?xml version="1.0" encoding="utf-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="StatSetting">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="GrowthByLevel">
          <xs:complexType>
            <xs:sequence>
              <xs:element maxOccurs="unbounded" name="Stat">
                <xs:complexType>
                  <xs:attribute name="type" type="xs:string" use="required" />
                  <xs:attribute name="value" type="xs:decimal" use="required" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="GrowthByEnchant">
          <xs:complexType>
            <xs:sequence>
              <xs:element maxOccurs="unbounded" name="Stat">
                <xs:complexType>
                  <xs:attribute name="type" type="xs:string" use="required" />
                  <xs:attribute name="value" type="xs:decimal" use="required" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="GrowthByLimitBreak">
          <xs:complexType>
            <xs:sequence>
              <xs:element maxOccurs="unbounded" name="Stat">
                <xs:complexType>
                  <xs:attribute name="type" type="xs:string" use="required" />
                  <xs:attribute name="value" type="xs:decimal" use="required" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="StatTraining">
          <xs:complexType>
            <xs:sequence>
              <xs:element maxOccurs="unbounded" name="Stat">
                <xs:complexType>
                  <xs:attribute name="type" type="xs:string" use="required" />
                  <xs:attribute name="value" type="xs:decimal" use="required" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="StatTrainingByLimitBreak">
          <xs:complexType>
            <xs:sequence>
              <xs:element maxOccurs="unbounded" name="Stat">
                <xs:complexType>
                  <xs:attribute name="type" type="xs:string" use="required" />
                  <xs:attribute name="value" type="xs:decimal" use="required" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="StatLimits">
          <xs:complexType>
            <xs:sequence>
              <xs:element maxOccurs="unbounded" name="Limit">
                <xs:complexType>
                  <xs:attribute name="type" type="xs:string" use="required" />
                  <xs:attribute name="minMulValue" type="xs:decimal" use="optional" />
                  <xs:attribute name="maxMulValue" type="xs:decimal" use="optional" />
                  <xs:attribute name="minValue" type="xs:decimal" use="optional" />
                  <xs:attribute name="maxValue" type="xs:decimal" use="optional" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>